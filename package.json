{
  "name": "@biggerstar/layout",
  "version": "0.0.8-Beta",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vite build",
    "preview": "vite preview",
    "bundle": "microbundle --entry src/index.js -f es,cjs,umd -o ./dist --sourcemap=false",
    "release": "npm run -s build && git commit -am $npm_package_version && git tag $npm_package_version && git push --tags && npm publish"
  },
  "main": "./src/index.js",
  "exports": {
    "default": "./dist/any-grid-layout",
    "import": "./dist/any-grid-layout",
    "require": "./dist/any-grid-layout.umd.cjs"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.0",
    "vite": "^3.1.0",
    "vue": "^3.2.45",
    "rimraf": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/biggerstar/any-grid-layout"
  },
  "author": "BiggerStar",
  "license": "GPL-3.0-or-later",
  "description": "",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
  },
  "eslintIgnore": [
    "dist"
  ]
}
